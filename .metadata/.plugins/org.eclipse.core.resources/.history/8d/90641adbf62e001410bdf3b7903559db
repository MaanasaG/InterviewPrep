	private static void find(int[] input) {
		
		HashSet<Integer> hash = new HashSet<Integer>();

	    for(int i = 0; i < input.length; i++){

	        for(int j = i+1; j< input.length; j++){

	            if(input[i] == input[j]){

	                hash.add(input[i]);

	            }
	        }
	        
	    }
	    
	    System.out.println(hash);

	}
	
	
}
