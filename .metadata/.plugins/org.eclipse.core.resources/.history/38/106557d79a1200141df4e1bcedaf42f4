package com.org.search;

public class SwapNoTemp {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
         int a=10,b=6;
         a=a+b;
         b=a;
         a=a-b;
         
         System.out.println("a="+a);
         System.out.println("b="+b);
		
		
	}

}
