package com.org.simplePbms;

public class CheckAnagrams {

	public static void main(String args[]){
		String first="Rian";
		String second = "inaR";
		checkAnagrams(first,second);
		
		
		
	}

	private static boolean checkAnagrams(String first, String second) {
		int sum1 = 0,sum2=0;
		boolean isAnagram=false;
		char[] firstCharArray = first.toCharArray();
		char[] secondCharArray= second.toCharArray();
		for (int i=0; i<firstCharArray.length;i++){
		sum1=sum1+firstCharArray[i];			
		}
		for (int j=0; j<secondCharArray.length;j++){
			sum2=sum2+firstCharArray[j];			
		}
		
		
		
		return isAnagram;
	}
	
	
}
