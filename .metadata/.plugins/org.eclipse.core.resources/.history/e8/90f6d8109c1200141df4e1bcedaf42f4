package com.org.search;

public class BubbleSort {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int[] list={8,9,11,2,4,6,0};
      int temp;
		for(int i=0;i<list.length;i++){
			if(list[i]<list[i+1]){
				temp=list[i];
				list[i]=list[i+1];
				list[i+1]=temp;
			}
		}
		
	}

}
