package com.org.logicalpbms;

public class MissingNumber {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] list={1,2,3,4,5,6,8,9,10};
        System.out.println(findMissingNumber(list,list.length));
          
	}

	private static int findMissingNumber(int[] list, int n) {
		int result = 0,t=0;
		int l=(n*(n+1))/2;
		System.out.println(l);
		for (int k=0;k<list.length;k++)
		{
			t=t+list[k];
			
		}
		System.out.println(t);
		result=l-t;
		
		return result;
	}

}
