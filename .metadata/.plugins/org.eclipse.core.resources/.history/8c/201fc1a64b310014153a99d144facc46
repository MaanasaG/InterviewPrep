package com.org.tests;


import org.junit.Test;

import com.org.strings.NonRepeatedCharacter;

public class NonRepeatedCharacterTest {

	@Test
	public void test() {
		String str1="c";
		String str2="Maanasa";
		String str3="abcdefg";
		String str4="";
		String str5=null;
		String str6="aaaaa";
		String str7="428367";
		String str8="$%^&**(*";


		NonRepeatedCharacter.findfirstNonrepeatedNumber(str1);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str2);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str3);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str4);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str6);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str7);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str8);
		NonRepeatedCharacter.findfirstNonrepeatedNumber(str5);

		
	}

}
