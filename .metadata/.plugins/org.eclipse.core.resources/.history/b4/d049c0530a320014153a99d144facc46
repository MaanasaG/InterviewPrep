			}

			private static void findTheFirstNonRepeatedNumver(int[] input) {
				HashSet<Integer> result = new HashSet<Integer>();
				for(int i=0; i<input.length;i++){
					if(result.add(input[i])==false){
						System.out.println(input[i]);
					}
				}
			}
}
