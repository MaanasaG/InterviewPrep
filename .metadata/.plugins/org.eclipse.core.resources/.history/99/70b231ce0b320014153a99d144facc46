package com.org.numbers;

public class FirstNonRepeatedNumber {

	/**
	 * @param args
	 */
	public static void main(String[] args) {	
		int[] input= {1,3,5,7,9,3};
		firstNonRepeatedNumber(input);

	}

	private static void firstNonRepeatedNumber(int[] input) {
		int[] charcount = new int[256];
		for(int i=0;i<input.length;i++){
			charcount[input[i]]++;
		}
		for(int i=0;i<input.length;i++){
			if(charcount[input.charAt(i)]==1){
				output.append(input.charAt(i));
				//output.append(charcount[input.charAt(i)]);
			}
	}

}
