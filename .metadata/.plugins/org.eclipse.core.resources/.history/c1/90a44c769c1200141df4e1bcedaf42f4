package com.org.search;

public class BubbleSort {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int[] list={8,9,11,2,4,6,0};
      int temp;
      boolean sorted;
		for(int i=0;i<list.length-1;i++){
			sorted=true;
			if(list[i]<list[i+1]){
				temp=list[i];
				list[i]=list[i+1];
				list[i+1]=temp;
			}
			System.out.print(list[i]);
			System.out.print(",");
		}
		
	}

}
